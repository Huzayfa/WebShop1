<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title </title>
    <link rel="shortcut icon" type="image/icon" src="~/favicon.ico" />
    <link href="~/fonts/ImageStyleSheet.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/fonts/Loader.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    
    <script src="~/Scripts/jquery-3.1.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.12.1.min.js"></script>
    <link href="~/Content/ToolTip.css" rel="stylesheet" />
    @*<script src="~/Scripts/jquery-1.10.2.min.js"></script>*@

    
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/angular-route.min.js"></script>
    <script src="~/Scripts/angular-cookies.min.js"></script>




</head>

<body ng-app="WebShop">




    @Html.Partial("_NavBarPartialView")
    <div class="body-content" ng-controller="AppCtrl">
        <div ng-show="Loading">
            <img  src="~/fonts/lampaLoader.gif"
                 class="img-circle imgloader" />
        </div>
      
        <div ng-show="Loading" class="loader">
            
                 
         </div>
        
        <div>
            @RenderBody()
        </div>
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My WebShop</p>
        </footer>
    </div>



    
    <script src="~/Angular-App/App/WebShop.js"></script>
    <script src="~/Angular-App/Service/CookieOptionService.js"></script>
    <script src="~/Angular-App/Factory/toJavaScriptDate.js"></script>
    <script src="~/Angular-App/Service/OrderServices.js"></script>
    <script src="~/Angular-App/Routing/WebShopRoute.js"></script>
    <script src="~/Angular-App/Controller/ShopUsersListCtrl.js"></script>
    <script src="~/Angular-App/Controller/RegisterUsersListCtrl.js"></script>
    <script src="~/Angular-App/Controller/ProductCustomerAllDetailsCtrl.js"></script>
    <script src="~/Angular-App/Controller/CreateShopUsersCtrl.js"></script>
    <script src="~/Angular-App/Controller/RegisterShopUsersCtrl.js"></script>

    <script src="~/Angular-App/Controller/ProductControlPanelListCtrl.js"></script>
    <script src="~/Angular-App/Controller/CategoryControlPanelListCtrl.js"></script>
    <script src="~/Angular-App/Controller/ShopCartCtrl.js"></script>
    <script src="~/Angular-App/Controller/ViewShopCartCtrl.js"></script>

    <script src="~/Angular-App/Controller/RecommendedProductCtrl.js"></script>
    <script src="~/Angular-App/Controller/OrderControlPanelListCtrl.js"></script>
    <script src="~/Angular-App/Filter/MyDateFilter.js"></script>
    <script src="~/Angular-App/Controller/LoginCtrl.js"></script>
    <script src="~/Angular-App/JavaScript/ViewShopCartFunctions.js"></script>
    <script src="~/Angular-App/JavaScript/SharedFunctions.js"></script>
    <script src="~/Angular-App/Filter/PagingList.js"></script>
    <script src="~/Angular-App/Service/PagingList.js"></script>
    <script src="~/Angular-App/Service/CartService.js"></script>
    <script src="~/Angular-App/Directive/ProductDetails.js"></script>
    <script src="~/Angular-App/Controller/AppCtrl.js"></script>
    <script src="~/Angular-App/Directive/OrderDetails.js"></script>
    @* try Google Maps*@
    @*google Map*@
    <script src="~/Angular-App/JavaScript/GoogleMaps.js"></script>


    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBm61-xZS8nyY0q_b1rEn34Kh2SmoXDdIQ&callback=initMap"></script>




    
    @*Including the style sheet*@
    <link href="~/fonts/AngularValidationStyle.css" rel="stylesheet" />
    <link href="~/fonts/tableStyleSheeet.css" rel="stylesheet" />
</body>
</html>